select sum(population) from city where district = "California";
select avg(population) from city where district = "California";
select round(avg(population), 0) from city;
select sum(population) from city where COUNTRYCODE = "jpn";
select max(population) - min(population) from city;
SELECT CEIL(AVG(CAST(salary AS DECIMAL)) - AVG(CAST(REPLACE(salary , '0', '') AS DECIMAL))) FROM EMPLOYEES;
SELECT earnings, COUNT(earnings) FROM ( SELECT months * salary AS earnings FROM employee ) AS earnings_table WHERE earnings = ( SELECT MAX(months * salary) FROM employee ) GROUP BY earning;
select count(city) - count(distinct city) from station;
select CITY, length(CITY) FROM STATION where length(city) = (select min(length(city)) from station) or length(city) = (select max(length(city)) from station) order by length(city) desc, city asc limit 2;
select round(sum(lat_n), 4) from station where lat_n > 38.788 and lat_n < 137.2345;
select round(max(lat_n), 4) from station where lat_n < 137.2345;
select round(max(long_w), 4) from station where lat_n = (select max(lat_n) from station where lat_n < 137.2345);
select round(min(lat_n), 4) from station where lat_n > 38.778;
select round(long_w, 4) from station where lat_n = (select min(lat_n) from station where lat_n > 38.788);
select round(abs(abs(min(lat_n) - max(lat_n)) + abs(min(long_w) - max(long_w))), 4) from station;
Select c.company_code, founder, count(Distinct lead_manager_code), count(Distinct senior_manager_code ), count(Distinct manager_code), count(Distinct employee_code) from Employee e Join Company c on e.company_code =c.company_code Group by c.company_code, founder Order by c.company_code;
select round(sqrt((min(lat_n) - max(lat_n)) * (min(lat_n) - max(lat_n)) + (min(long_w) - max(long_w)) * (min(long_w) - max(long_w))), 4) from station;